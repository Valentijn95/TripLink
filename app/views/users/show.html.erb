<h1>User show page</h1>
<h2><%= @user.name %></h2>
<p><%= @user.guide_description %></p>
  <% if @user.photo.key != nil %>
    <%= cl_image_tag @user.photo.key, width: 400, crop: :fill %>
  <% end %>
<br>
<p><%= link_to "Edit this user", edit_user_registration_path %></p>
<p><%= link_to "Go back to /users", users_path %></p>
